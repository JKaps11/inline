{
  "name": "inline",
  "displayName": "Inline Notes",
  "description": "An efficient and quick note taking app for your organizational needs in a project",
  "version": "0.1.0",
  "license": "MIT",
  "keywords": [
    "notes",
    "inline",
    "todo",
    "task"
  ],
  "publisher": "JKaps11",
  "icon": "TODO do a icon here",
  "repository": "https://github.com/JKaps11/inline",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "inline.file.add",
        "title": "$Add File"
      },
      {
        "command": "inline.menu.item.delete",
        "title": "Delete Item"
      },
      {
        "command": "inline.note.edit.name",
        "title": "Edit Name"
      }
    ],
    "viewsWelcome": [
      {
        "view": "inline_notes_view",
        "contents": "Start by opening a workspace and creating a note with the plus icon above!"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "inline_view_container",
          "title": "Inline",
          "icon": "images/notebook-icon.svg"
        }
      ]
    },
    "views": {
      "inline_view_container": [
        {
          "id": "inline_notes_view",
          "name": "Inline Notes",
          "contextualTitle": "Inline Notes",
          "icon": "images/notebook-icon.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "inline.file.add",
          "when": "view == inline_notes_view"
        }
      ],
      "view/item/context": [
        {
          "command": "inline.note.edit.name",
          "when": "view == inline_notes_view",
          "group": "inline"
        },
        {
          "command": "inline.menu.item.delete",
          "when": "view == inline_notes_view"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.106.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {
    "dayjs": "^1.11.19"
  }
}